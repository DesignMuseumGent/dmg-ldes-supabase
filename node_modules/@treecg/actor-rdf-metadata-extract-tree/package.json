{
  "name": "@treecg/actor-rdf-metadata-extract-tree",
  "version": "2.0.0",
  "description": "A tree rdf-metadata-extract actor",
  "lsd:module": true,
  "main": "index.js",
  "typings": "index",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "files": [
    "components",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "devDependencies": {
    "componentsjs-generator": "^3.1.0"
  },
  "dependencies": {
    "@comunica/bus-rdf-metadata-extract": "^2.3.0",
    "@comunica/core": "^2.3.0",
    "@treecg/tree-metadata-extraction": "^1.2.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "test/tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "test": "node \"../../node_modules/jest/bin/jest.js\" ${1}",
    "test-watch": "node \"../../node_modules/jest/bin/jest.js\" ${1} --watch",
    "lint": "node \"../../node_modules/tslint/bin/tslint\" lib/**/*.ts test/**/*.ts --exclude '**/*.d.ts'",
    "build": "tsc && npm run build:components",
    "build:components": "componentsjs-generator",
    "validate": "npm ls"
  },
  "gitHead": "dd632f5cfec133f8ad32a38508da100f37aee705"
}
